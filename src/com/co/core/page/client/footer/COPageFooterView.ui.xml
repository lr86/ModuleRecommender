<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:gxt='urn:import:com.sencha.gxt.widget.core.client'
	xmlns:gxtContainer='urn:import:com.sencha.gxt.widget.core.client.container'
	xmlns:gxtButton='urn:import:com.sencha.gxt.widget.core.client.button'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:with field='resources' type='com.co.core.page.shared.resources.images.COPageImageResources' />

	<g:VerticalPanel width="100%">
		<gxt:ContentPanel headerVisible="false">
			<g:HorizontalPanel width="100%" verticalAlignment="ALIGN_MIDDLE" spacing="2">
				<g:Cell verticalAlignment="ALIGN_MIDDLE">
					<g:SimplePanel ui:field="footerLeftContainer">
						<g:Label ui:field="lbCopyright"></g:Label>
					</g:SimplePanel>
				</g:Cell>
				<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
					<g:SimplePanel ui:field="footerCenterContainer">
						<g:Label></g:Label>
					</g:SimplePanel>
				</g:Cell>
				<g:Cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE">
					<g:SimplePanel ui:field="footerRightContainer">
						<g:Anchor ui:field="aSupport"></g:Anchor>
					</g:SimplePanel>
				</g:Cell>
			</g:HorizontalPanel>
		</gxt:ContentPanel>
	</g:VerticalPanel>
</ui:UiBinder>