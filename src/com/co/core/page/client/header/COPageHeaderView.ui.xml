<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:gxt='urn:import:com.sencha.gxt.widget.core.client'
	xmlns:gxtContainer='urn:import:com.sencha.gxt.widget.core.client.container'
	xmlns:gxtButton='urn:import:com.sencha.gxt.widget.core.client.button'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:VerticalPanel width="100%" height="100%">
		<gxt:FramedPanel headerVisible="false">
			<g:HorizontalPanel width="100%"  verticalAlignment="ALIGN_MIDDLE" spacing="2">
				<g:Cell verticalAlignment="ALIGN_MIDDLE">
					<g:SimplePanel ui:field="headerLeftContainer">
					</g:SimplePanel>
				</g:Cell>
				<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
					<g:SimplePanel ui:field="headerCenterContainer">
					</g:SimplePanel>
				</g:Cell>
				<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
					<g:SimplePanel ui:field="headerCenterRightContainer">
					</g:SimplePanel>
				</g:Cell>
				<g:Cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE">
					<g:SimplePanel ui:field="headerRightContainer">
					</g:SimplePanel>
				</g:Cell>
			</g:HorizontalPanel>
		</gxt:FramedPanel>
	</g:VerticalPanel>
</ui:UiBinder>