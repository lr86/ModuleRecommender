<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:gxt='urn:import:com.sencha.gxt.widget.core.client'
	xmlns:gxtContainer='urn:import:com.sencha.gxt.widget.core.client.container'
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>



	<g:DockLayoutPanel unit='EM'>
	
		<g:north size='4'>
			<g:SimpleLayoutPanel width="100%" ui:field="headerContentPanel">
			</g:SimpleLayoutPanel>
		</g:north>

		<g:south size='2'>
			<g:SimpleLayoutPanel width="100%" ui:field="footerContentPanel">
			</g:SimpleLayoutPanel>
		</g:south>

		<g:center>
			<g:SimpleLayoutPanel width="100%" height="100%" ui:field="mainContentPanel">
			</g:SimpleLayoutPanel>
		</g:center>
		
	</g:DockLayoutPanel>

</ui:UiBinder>
